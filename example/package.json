{
  "name": "app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild",
    "android": "expo run:android --variant mobileDebug",
    "quest": "expo run:android --variant questDebug",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules && rm -rf android && rm -rf ios",
    "postinstall": "patch-package"
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "ts:check": "tsc -p . --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "expo": "54.0.13",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-linking": "~8.0.8",
    "expo-quest": "file:../expo-quest",
    "expo-quest-location": "file:../expo-quest-location",
    "expo-quest-notifications": "file:../expo-quest-notifications",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "@react-native/eslint-config": "^0.82.0",
    "eslint": "^8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
